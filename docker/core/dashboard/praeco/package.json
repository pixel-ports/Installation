    "@fortawesome/free-regular-svg-icons": "5.3.1",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@fortawesome/vue-fontawesome": "0.1.1",
    "@riophae/vue-treeselect": "0.0.37",
    "axios": "0.18.0",
    "babel-core": "6.26.3",
    "browser-bunyan": "1.4.0",
    "change-case": "3.0.2",
    "comma-number": "2.0.0",
