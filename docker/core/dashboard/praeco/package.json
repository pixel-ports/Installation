    "normalize.css": "8.0.0",
    "prettycron": "0.10.0",
    "prismjs": "1.15.0",
    "semver": "5.6.0",
    "string-format": "2.0.0",
    "valid-url": "1.0.9",
    "vue": "2.5.18",
    "vue-at": "2.5.0-beta",
    "vue-echarts": "3.1.2",
    "vue-infinite-scroll": "2.0.2",
    "vue-json-pretty": "1.4.1",
    "vue-native-websocket": "2.0.11",
    "vue-prism-component": "1.0.1",
    "vue-query-builder": "0.6.1",
    "vue-router": "3.0.1",
